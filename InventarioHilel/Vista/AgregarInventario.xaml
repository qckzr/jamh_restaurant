        </Button>
        <Label Content="Seleccionar fecha de vencimiento:" FontSize="28" FontWeight="Normal" Height="47" HorizontalAlignment="Left" Margin="93,358,0,0" Name="label2" VerticalAlignment="Top" Width="429" />
        <Label Content="Seleccionar Producto:" FontSize="28" FontWeight="Normal" Height="47" HorizontalAlignment="Left" Margin="234,206,0,0" Name="label3" VerticalAlignment="Top" Width="286" />
        <Label Content="Cantidad:" FontSize="28" FontWeight="Normal" Height="47" HorizontalAlignment="Left" Margin="379,520,0,0" Name="label4" VerticalAlignment="Top" Width="143" />
        <ListBox Height="154" HorizontalAlignment="Left" Margin="537,162,0,0" Name="listBoxProductos" VerticalAlignment="Top" Width="293">
            <ListBoxItem />
        </ListBox>
        <Rectangle Height="39" HorizontalAlignment="Right" Margin="0,528,586,0" Name="rectangle1" Stroke="Black" VerticalAlignment="Top" Width="189" />
        <Calendar Height="180" HorizontalAlignment="Left" Margin="576,342,0,0" Name="calendar1" VerticalAlignment="Top" Width="216" />
        <Button Content="Aceptar" FontSize="26" Height="50" HorizontalAlignment="Left" Margin="591,587,0,0" Name="b_agregarInventario" VerticalAlignment="Top" Width="189" />
    </Grid>
</Window>
